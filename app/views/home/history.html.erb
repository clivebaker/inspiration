<div class="row">
    <div class="col">
        <table class="table table-striped table-dark table-sm">
            <thead>
                <tr>
                    <th>Date</th>
                   <th>Time</th>
                   <th>Which One</th>
                    <th>Rating</th>
                    <th>Quote</th>
                    <th>Author</th>
                </tr>
            </thead>
            <tbody>
                <% @logs.each do |log| %>
                <tr>
                    <td class='montserrat-thin small'><%= log.created_at.strftime('%d %b %y')%></td>
         
                    <td class='montserrat-thin small'><%= log.created_at.strftime('%H:%M')%></td>
                               <td class='montserrat-thin small'><%= log.token%></td>
                    <td><% (1..log.rating).each do %><i class="fa-sharp-duotone fa-light fa-star small text-warning"></i><% end %></td>
                    <td class='montserrat-thin small'><%= log.quote_data%></td>
                   <td class='montserrat-thin-italic small'><%= log.quote_author%></td>
                </tr>
                <% end %>
            </tbody>
        </table>
    </div>
</div>